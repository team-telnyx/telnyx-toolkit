# Your Telnyx API key (get one at portal.telnyx.com → Keys & Credentials)
TELNYX_API_KEY=KEYxxxxx

# Storage bucket name (create at portal.telnyx.com → Storage)
BACKUP_BUCKET=openclaw-backup

# Storage region
BACKUP_REGION=us-central-1

# Path to your OpenClaw workspace
OPENCLAW_WORKSPACE=/home/user/clawd

# How many backups to keep (oldest are deleted first)
MAX_BACKUPS=48
